/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in= a , out= notA);
    Not(in= b, out= notB);
    And(a= notA, b= b, out= temp1);
    And(a= a, b= notB, out= temp2);
    Or(a= temp1, b= temp2, out= out);
}
